{
  "name": "react-form-addons",
  "version": "3.0.2",
  "description": "Compose simple and nested forms with Higher-order components / decorators in React",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir .",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf lib components redux utils coverage .nyc_output",
    "deploy-gh-pages": "cd site && yarn install && yarn run deploy",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --ext js src tests",
    "test": "cross-env NODE_PATH=. NODE_ENV=test jest --coverage"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeojz/react-form-addons.git"
  },
  "homepage": "https://yeojz.github.io/react-form-addons",
  "keywords": [
    "react",
    "reactjs",
    "form",
    "forms",
    "nested-forms",
    "addons",
    "decorator",
    "higher-order",
    "component",
    "hoc",
    "builder",
    "factory",
    "composer",
    "state",
    "redux"
  ],
  "author": "Gerald Yeo <contact@fusedthought.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yeojz/react-form-addons/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.22.0",
    "coveralls": "^2.11.15",
    "cross-env": "^5.0.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.13.1",
    "eslint-plugin-react": "^6.9.0",
    "jest": "^19.0.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4",
    "sinon": "^2.1.0"
  },
  "dependencies": {
    "immutability-helper": "^2.1.1",
    "invariant": "^2.2.2",
    "lodash": "^4.0.0",
    "prop-types": "^15.5.4"
  }
}
